          </property>
        </property>
        <property>
          <type name="Function" />
          <desc/>
          <argument name="" type="" return="">
            <desc/>
          </argument>
        </property>
      </parameter>
      
      <!-- see Function type for argument.option.@type == function -->
      <parameter name="">
        <type name="Function" />
			</parameter>
			
    </option>
    
    <option default="value|auto" default-note="" name="">
      <type name="Function" />
